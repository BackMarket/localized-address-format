{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "moduleResolution": "node",
        "lib": [
            "esnext"
        ],
        "declaration": true,
        "outDir": "./lib",
        "strict": true,
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "downlevelIteration": true
    },
    "include": ["src"],
    "exclude": ["node_modules", "**/__tests__/*"]
}
